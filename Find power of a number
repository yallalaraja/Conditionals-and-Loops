import java.util.Scanner;
public class Solution {
    
    public static void main(String[] args) {
        // Write your code here
        Scanner sc = new Scanner(System.in);
        int base = sc.nextInt();
        int exp =  sc.nextInt();
        int res = 1;
        while(exp!=0)
        {
            res = res*base;
            exp--;
        }
        System.out.print(res);
    }
}
